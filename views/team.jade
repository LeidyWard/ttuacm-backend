extends layout

block content
    
    // Roll the Credits!
    div.row.contentSection#team
        div.col-sm-10.col-sm-offset-1.col-xs-12
            each person, i in teamMembers
                div.col-sm-4.team.col-xs-12
                    h4 #{teamMembers[i].firstName} #{teamMembers[i].lastName}
                    h5: i #{teamMembers[i].position}
