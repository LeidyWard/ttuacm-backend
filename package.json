    "cors": "~2.8.4",
    "debug": "~2.6.3",
    "dotenv": "~5.0.1",
    "express": "~4.16.3",
    "google-auth-library": "~0.10.0",
    "googleapis": "^33.0.0",
    "jsonwebtoken": "~8.2.1",
    "jwt-decode": "~2.2.0",
    "mongoose": "~4.13.12",
    "morgan": "~1.9.0",
