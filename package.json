{
  "name": "ttuacm-backend",
  "version": "2.0.0",
  "license": "MIT",
  "description": "ACM TTU Backend",
  "scripts": {
    "deploy-test": "firebase deploy --only functions:test",
    "deploy-helloWorld": "firebase deploy --only functions:helloWorld",
    "deploy": "firebase deploy --only functions",
    "serve": "nodemon --exec firebase serve --only functions",
    "lint": "eslint auth contacts email events profile",
    "set-env": "firebase functions:config:set",
    "env-config": "firebase functions:config:get > .runtimeconfig.json",
    "start": "yarn serve",
    "logs": "firebase functions:log",
    "docs": "esdoc -c esdoc.json"
  },
  "dependencies": {
    "axios": "~0.18.0",
    "bcryptjs": "~2.0",
    "body-parser": "~1.18.3",
    "cors": "~2.8.4",
    "express": "4.16.3",
    "firebase-admin": "~6.0.0",
    "firebase-functions": "2.0.3",
    "googleapis": "33.0.0",
    "mongoose": "~4.13.12",
    "passport": "~0.4.0",
    "passport-facebook": "~2.1.1",
    "passport-github": "~1.1.0",
    "passport-google-oauth2": "~0.1.6",
    "passport-jwt": "~4.0.0"
  },
  "private": true,
  "engines": {
    "node": "8"
  },
  "devDependencies": {
    "cross-env": "~5.2.0",
    "eslint": "5.6.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "firebase-functions-test": "0.1.3",
    "jest": "23.6.0",
    "nodemon": "^1.18.3",
    "sinon": "~6.0.0"
  }
}
